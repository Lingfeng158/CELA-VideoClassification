@Article{	  Thomas:2011:HMO,
  author	= {Thomas, Laura Elizabeth and Seiffert, Adriane E},
  title		= {How many objects are you worth? Quantification of the
		  self-motion load on multiple object tracking},
  journal	= {Frontiers in Psychology},
  volume	= {2},
  year		= {2011},
  number	= {245},
  url		= {http://www.frontiersin.org/cognition/10.3389/fpsyg.2011.00245/abstract},
  doi		= {10.3389/fpsyg.2011.00245},
  issn		= {1664-1078},
  abstract	= {Perhaps walking and chewing gum is effortless, but walking
		  and tracking moving objects is not. Multiple object
		  tracking is impaired by walking from one location to
		  another, suggesting that updating location of the self puts
		  demands on object tracking processes. Here, we quantified
		  the cost of self-motion in terms of the tracking load.
		  Participants in a virtual environment tracked a variable
		  number of targets (1-5) among distractors while either
		  staying in one place or moving along a path that was
		  similar to the objects motion. At the end of each trial,
		  participants decided whether a probed dot was a target or
		  distractor. As in our previous work, self-motion
		  significantly impaired performance in tracking multiple
		  targets. Quantifying tracking capacity for each individual
		  under move versus stay conditions further revealed that
		  self-motion during tracking produced a cost to capacity of
		  about 0.8 (±0.2) objects. Tracking your own motion is
		  worth about one object, suggesting that updating the
		  location of the self is similar, but perhaps slightly
		  easier, than updating locations of objects.}
}

